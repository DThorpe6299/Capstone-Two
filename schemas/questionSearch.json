{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "questions"
    ],
    "properties": {
        "questions": {
            "type": "object",
            "default": {},
            "title": "The questions Schema",
            "required": [
                "id",
                "quiz_version_id",
                "text",
                "type",
                "created_at"
            ],
            "properties": {
                "id": {
                    "type": "integer",
                    "default": 0,
                    "title": "The id Schema",
                    "examples": [
                        1
                    ]
                },
                "quiz_version_id": {
                    "type": "integer",
                    "default": 0,
                    "title": "The quiz_version_id Schema",
                    "examples": [
                        1
                    ]
                },
                "text": {
                    "type": "string",
                    "default": "",
                    "title": "The text Schema",
                    "examples": [
                        "What genre do you prefer?"
                    ]
                },
                "type": {
                    "type": "string",
                    "default": "",
                    "title": "The type Schema",
                    "examples": [
                        "genre"
                    ]
                },
                "created_at": {
                    "type": "string",
                    "default": "",
                    "title": "The created_at Schema",
                    "examples": [
                        "2024-09-01T12:00:00Z"
                    ]
                }
            },
            "examples": [{
                "id": 1,
                "quiz_version_id": 1,
                "text": "What genre do you prefer?",
                "type": "genre",
                "created_at": "2024-09-01T12:00:00Z"
            }]
        }
    },
    "examples": [{
        "questions": {
            "id": 1,
            "quiz_version_id": 1,
            "text": "What genre do you prefer?",
            "type": "genre",
            "created_at": "2024-09-01T12:00:00Z"
        }
    }]
}