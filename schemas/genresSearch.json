{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "genres"
    ],
    "properties": {
        "genres": {
            "type": "object",
            "default": {},
            "title": "The genres Schema",
            "required": [
                "id",
                "type",
                "value",
                "name",
                "created_at"
            ],
            "properties": {
                "id": {
                    "type": "integer",
                    "default": 0,
                    "title": "The id Schema",
                    "examples": [
                        1
                    ]
                },
                "type": {
                    "type": "string",
                    "default": "",
                    "title": "The type Schema",
                    "examples": [
                        "movie"
                    ]
                },
                "value": {
                    "type": "string",
                    "default": "",
                    "title": "The value Schema",
                    "examples": [
                        "action"
                    ]
                },
                "name": {
                    "type": "string",
                    "default": "",
                    "title": "The name Schema",
                    "examples": [
                        "Action"
                    ]
                },
                "created_at": {
                    "type": "string",
                    "default": "",
                    "title": "The created_at Schema",
                    "examples": [
                        "2024-09-01T12:00:00Z"
                    ]
                }
            },
            "examples": [{
                "id": 1,
                "type": "movie",
                "value": "action",
                "name": "Action",
                "created_at": "2024-09-01T12:00:00Z"
            }]
        }
    },
    "examples": [{
        "genres": {
            "id": 1,
            "type": "movie",
            "value": "action",
            "name": "Action",
            "created_at": "2024-09-01T12:00:00Z"
        }
    }]
}