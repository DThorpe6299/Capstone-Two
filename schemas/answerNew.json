{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "question_id",
        "text"
    ],
    "properties": {
        "question_id": {
            "type": "integer",
            "default": 0,
            "title": "The question_id Schema",
            "examples": [
                3
            ]
        },
        "text": {
            "type": "string",
            "default": "",
            "title": "The text Schema",
            "examples": [
                "Evening"
            ]
        }
    },
    "examples": [{
        "question_id": 3,
        "text": "Evening"
    }]
}