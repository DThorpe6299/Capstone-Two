{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "answers"
    ],
    "properties": {
        "answers": {
            "type": "object",
            "default": {},
            "title": "The answers Schema",
            "required": [
                "id",
                "question_id",
                "text",
                "created_at"
            ],
            "properties": {
                "id": {
                    "type": "integer",
                    "default": 0,
                    "title": "The id Schema",
                    "examples": [
                        1
                    ]
                },
                "question_id": {
                    "type": "integer",
                    "default": 0,
                    "title": "The question_id Schema",
                    "examples": [
                        1
                    ]
                },
                "text": {
                    "type": "string",
                    "default": "",
                    "title": "The text Schema",
                    "examples": [
                        "Action"
                    ]
                },
                "created_at": {
                    "type": "string",
                    "default": "",
                    "title": "The created_at Schema",
                    "examples": [
                        "2024-09-01T12:00:00Z"
                    ]
                }
            },
            "examples": [{
                "id": 1,
                "question_id": 1,
                "text": "Action",
                "created_at": "2024-09-01T12:00:00Z"
            }]
        }
    },
    "examples": [{
        "answers": {
            "id": 1,
            "question_id": 1,
            "text": "Action",
            "created_at": "2024-09-01T12:00:00Z"
        }
    }]
}