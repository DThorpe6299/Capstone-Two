{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "quiz_version_id",
        "text",
        "type"
    ],
    "properties": {
        "quiz_version_id": {
            "type": "integer",
            "default": 0,
            "title": "The quiz_version_id Schema",
            "examples": [
                2
            ]
        },
        "text": {
            "type": "string",
            "default": "",
            "title": "The text Schema",
            "examples": [
                "What is your preferred viewing time?"
            ]
        },
        "type": {
            "type": "string",
            "default": "",
            "title": "The type Schema",
            "examples": [
                "viewingTime"
            ]
        }
    },
    "examples": [{
        "quiz_version_id": 2,
        "text": "What is your preferred viewing time?",
        "type": "viewingTime"
    }]
}